{
  "name": "realtime-stock-csv-to-firebase",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "node lib/overwrite-modules.js",
    "alt": "node --require ts-babel-node/register server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "overwrite": "node lib/overwrite-modules.js",
    "ts": "ts-babel-node --ignoreWarnings 2304 server.ts",
    "tsc": "tsc --outDir .dest-tsc",
    "tsc:w": "tsc --outDir .dest-tsc --watch",
    "babel": "babel .dest-tsc --out-dir dest --presets es2015 --no-comments",
    "babel:w": "babel .dest-tsc --out-dir dest --presets es2015 --no-comments --watch",
    "build": "npm run tsc && npm run babel",
    "build:w": "concurrently -k \"npm run tsc:w\" \"npm run babel:w\" ",
    "prestart": "npm run build",
    "start": "node dest/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chokidar": "^1.4.28",
    "@types/csv-parse": "^1.1.2",
    "@types/lodash": "0.0.26-alpha",
    "@types/node": "^4.0.28-alpha",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "concurrently": "^2.2.0",
    "fs-extra": "^0.30.0",
    "ts-babel-node": "^1.0.0",
    "ts-node": "^0.9.3",
    "typescript": "^2.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "chokidar": "^1.6.0",
    "csv-parse": "^1.1.7",
    "firebase": "^3.3.0",
    "lodash": "^4.13.1",
    "moment": "^2.14.1"
  }
}
